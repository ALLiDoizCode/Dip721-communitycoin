type TxReceipt = 
 variant {
   Err:
    variant {
      AmountTooSmall;
      BlockUsed;
      ErrorOperationStyle;
      ErrorTo;
      InsufficientAllowance;
      InsufficientBalance;
      LedgerTrap;
      Other: text;
      Unauthorized;
    };
   Ok: nat;
 };
type Holder = 
 record {
   amount: nat;
   holder: principal;
   receipt: TxReceipt;
 };
service : {
  burnFee: (float64) -> ();
  devFee: (float64) -> ();
  distribute: (nat, vec Holder) -> ();
  marketingFee: (float64) -> ();
}
